<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.user.find_price.MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/candleHolder"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Candle holder"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/chowki"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Chowki"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/coaster"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Coaster"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/drawer"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Chest of Drawers"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/frame"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Frame"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/polygonBox"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Polygon Box"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/hutBox"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Hut box"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/penHolder"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Pen holder"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/simpleBox"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Simple Box"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/tray"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Tray"
            android:textAllCaps="true"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"></View>
        <TextView
            android:id="@+id/tissueBox"
            style="@style/CategoryStyle"
            android:background="#e3c0ac"
            android:text="Tissue Box"
            android:textAllCaps="true"/>


    </LinearLayout>
</ScrollView>